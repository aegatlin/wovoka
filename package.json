{
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir pages",
    "format": "prettier --write pages lib",
    "prepare": "skooh",
    "test": "playwright test"
  },
  "hooks": {
    "pre-commit": "npm run lint && npm run format && npm run test"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.35.0",
    "lucide-react": "^0.72.0",
    "next": "^12.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.23.1",
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "autoprefixer": "^10.4.4",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "skooh": "^0.2.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  }
}
